{
  "name": "@circlesac/cgrok",
  "version": "0.0.2",
  "type": "module",
  "description": "ðŸš€ An ngrok-like CLI that uses Cloudflare for secure tunneling",
  "keywords": ["cli", "tunnel", "cloudflare", "ngrok", "proxy"],
  "main": "dist/main.js",
  "bin": {
    "cgrok": "dist/main.js"
  },
  "files": ["dist"],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "barrels && bun lint && tsc --noEmit && bun run build.ts",
    "dev": "bun run src/main.ts",
    "start": "bun run build && bun run dist/main.js",
    "lint": "bun lint:eslint && bun lint:prettier && bun lint:biome",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier '**/*.{ts,tsx,js,jsx,json,md,cjs,mjs,mts,yml,yaml}' --write",
    "lint:biome": "biome check --write",
    "test": "vitest run --coverage",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@tszen/trycatch": "^0.1.0",
    "@types/js-yaml": "^4.0.9",
    "chalk": "^5.6.0",
    "cloudflare": "^4.5.0",
    "commander": "^14.0.0",
    "inquirer": "^12.9.4",
    "js-yaml": "^4.1.0",
    "moment-timezone": "^0.6.0",
    "nanospinner": "^1.2.2",
    "tldts": "^7.0.12",
    "tty-table": "^4.2.3"
  },
  "devDependencies": {
    "@piondev/shared-configs": "^1.0.35",
    "@tsed/barrels": "^6.6.3",
    "@types/bun": "^1.2.21",
    "@types/inquirer": "^9.0.9",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "3.2.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "license": "MIT"
}
